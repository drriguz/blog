<div class="blog-item-container columns">
    <div class="column is-one-fifth is-hidden-mobile">
        <%- partial('_partial/_leftbar') %>
    </div>
    <div class="column">
        <% page.posts.each(function (post) { %>
            <%- partial('_partial/_post', {post}) %>
            <% }) 
        %>
        <%- partial('_partial/_paginator') %>
    </div>
    <div class="column is-one-quarter is-hidden-mobile">
        <%- partial('_partial/_recommended') %>
        <%- partial('_partial/_tags') %>
    </div>
</div>
